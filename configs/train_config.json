{
  "seed": 7,
  "data": {
    "data_dict_path": "uniref50_gpt_data.pkl",
    "data_df_path": "uniref50_gzip_subsample.csv",
    "split_ratios": [0.88, 0.02, 0.1]
  },
  "datamodule": {
    "batch_size": 4,
    "max_protein_length": 750
  },
  "model": {
    "language_model": "gpt2-xl",
    "protein_model": "esm2_t12_35M_UR50D",
    "protein_layer_to_use": -1,
    "perceiver_latent_size": 20,
    "num_perceiver_layers": 1,
    "enable_gradient_checkpointing": false
  },
  "wandb": {
    "project": "Cprt-Experiments"
  },
  "trainer": {
    "precision": "16-mixed",
    "val_check_interval": 500,
    "max_epochs": 1,
    "devices": 1
  }
}
