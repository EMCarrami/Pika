{
  "seed": 42,
  "datamodule": {
    "data_dict_path": "uniref50_gpt_data.pkl",
    "split_ratios": [0.945, 0.005, 0.05],
    "max_protein_length": 1500,
    "max_text_length": 250,
    "data_field_names": "qa",
    "sequence_placeholder": "<protein sequence placeholder> ",
    "subsample_data": 1.0,
    "train_batch_size": 8,
    "eval_batch_size": 32,
    "num_workers": 4
  },
  "model": {
    "language_model": "gpt2-medium",
    "protein_model": "esm2_t12_35M_UR50D",
    "multimodal_strategy": "llm_only_baseline"
  },
  "wandb": {
    "project": "Cprt-Paper-Classification"
  }
}
